{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 500,
    "title": "The total number of car accidents in each Australian  state from 2010 to the present",
    "projection": {"type": "equirectangular"},
    "layer": [
      {
        "data": {
          "url":
          "https://raw.githubusercontent.com/wzen0014/3179/main/map_au_state.json",
          "format": {"type": "topojson", "feature":"ne_10m_admin_1_states_provinces"}
        },
      "mark": {"type": "geoshape", "fill": "#b1d7f7", "stroke":"black"}
      },
      {
        "data": {
          "url":
          "https://raw.githubusercontent.com/wzen0014/3179/main/map_au_state.json",
          "format": {"type": "topojson", "feature":"ne_110m_graticules_15"}
        },
        "mark": {"type": "geoshape", "stroke":"black"}
      },

      {
        "data": {
          "url":
          "https://raw.githubusercontent.com/wzen0014/3179/main/count.csv"
        },
        "mark": {"type": "circle","tooltip": {"content": "encoding"}},
        "encoding": {
          "color":{
            "field":"State",
            "type":"nominal",
            "title":"State",
            "scale":{"scheme": "category10"}
          },
        "description": {"field" : "Count", "type": "quantitative"},
        "longitude": {"field": "Longitude", "type":"quantitative"},
        "latitude": {"field": "Latitude", "type":"quantitative"}
        }
      }
      
    ]
  }